{
  "name": "2048",
  "version": "1.0.0",
  "description": "Прекрасная игра 2048 !",
  "main": "webpack.config.ts",
  "scripts": {
    "clean": "rimraf dist",
    "styleLint": "cross-env npx stylelint 'src/**/*.pcss'",
    "styleLintFix": "cross-env npx stylelint 'src/**/*.pcss' --fix",
    "eslint": "cross-env eslint 'src/**/*.{ts,tsx}'",
    "eslintFix": "cross-env eslint --fix 'src/**/*.{ts, tsx}'",
    "test": "cross-env jest",
    "build": "NODE_ENV=production NODE_PATH=. npm run clean && webpack --mode production",
    "dev:webpack": "cross-env webpack --env development --mode development --watch",
    "dev:express": "cross-env nodemon index.ts --watch dist/server.js",
    "dev": "NODE_ENV=development npm-run-all --print-label --parallel dev:*",
    "start": "NODE_ENV=production npm run build && cross-env nodemon index.ts",
    "composeReBuild": "docker-compose rm -f && docker-compose",
    "composeDev": "NODE_ENV=development docker-compose --env-file ./.env.development up",
    "composeProd": "NODE_ENV=production docker-compose --env-file ./.env.production up",
    "prepare": "cross-env husky install",
    "createNginxConfig": "node ./scripts/createNginxConfig.js",
    "pre-commit": "cross-env eslint 'src/**/*.{ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yandex-12-Team007/2048.git"
  },
