.nav {
  box-sizing: border-box;

  width: 100%;
  height: 60px;

  display: flex;
  justify-content: center;
  align-items: center;

  .nav__link {
    box-sizing: border-box;

    display: flex;
    justify-content: center;
    align-items: center;

    border: 1px solid red;

    margin: 10px;
  }
}
