.header {
  position: relative;
  box-sizing: border-box;
  padding: 0 10px 0 0;
  height: 65px;
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font: var(--title-font);
  color: var(--orange);
  border-bottom: 1px solid var(--gray);
}

.header-title {
  position: absolute;
  text-align: center;
  height: 64px;
  line-height: 64px;
  width: 100%;
}

.header-profile {
  width: 200px;
}
